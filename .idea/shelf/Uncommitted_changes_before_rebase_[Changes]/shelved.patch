ver {\n  transform: translateY(-2px);\n  text-shadow: 0 0 10px rgba(239, 237, 208, 0.5);\n}\n\n.text a {\n  color: inherit;\n  text-decoration: none;\n  position: relative;\n  transition: all 0.3s ease;\n}\n\n.text a::before {\n  content: '';\n  position: absolute;\n  bottom: -2px;\n  left: 0;\n  width: 100%;\n  height: 1px;\n  background-color: currentColor;\n  transform: scaleX(0);\n  transition: transform 0.3s ease;\n  transform-origin: right;\n}\n\n.text a:hover::before {\n  transform: scaleX(1);\n  transform-origin: left;\n}\n\n/* Pulsing highlight effect */\n@keyframes pulse {\n  0% {\n    opacity: 1;\n  }\n\n  50% {\n    opacity: 0.7;\n  }\n\n  100% {\n    opacity: 1;\n  }\n}\n\n.text span:nth-child(1) {\n  animation: pulse 4s infinite;\n}\n\n.text span:nth-child(2) {\n  animation: pulse 4s infinite 1s;\n}\n\n.text span:nth-child(3) {\n  animation: pulse 4s infinite 2s;\n}\n\n.text span:nth-child(4) {\n  animation: pulse 4s infinite 3s;\n}\n\n@keyframes scrollText {\n  from {\n    transform: translateX(100%);\n  }\n\n  to {\n    transform: translateX(-100%);\n  }\n}\n\n/* Hero section */\n.hero-section {\n  position: relative;\n  width: 100%;\n  height: 100%;\n  overflow: hidden;\n  min-height: 100vh;\n}\n\n.hero-container {\n  max-width: 1400px;\n  margin: 0 auto;\n  padding-left: 50px;\n  position: relative;\n  height: 100%;\n  display: flex;\n}\n\n.hero-content {\n  flex: 0 0 50%;\n}\n\n.hero-content h1 {\n  font-family: 'Lansdowne Demo', sans-serif;\n  font-size: 3.5rem;\n  line-height: 1.2;\n  margin-bottom: 1.5rem;\n  color: #efedd0;\n  font-weight: 400;\n  padding-top: 20%;\n}\n\n.hero-date {\n  font-family: 'Lato', sans-serif;\n  font-size: 18px !important;\n  color: #97eb61 !important;\n  margin-bottom: 2rem;\n  display: block;\n}\n\n.hero-btn {\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  width: 250px;\n  padding: 0.7em 1.4em;\n  color: white;\n  background: var(\n    --Gradient,\n    linear-gradient(180deg, #97eb6