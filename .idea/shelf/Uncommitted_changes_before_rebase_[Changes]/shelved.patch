ansition: background 0.3s ease;\n  border: none;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\n}\n\n.home-btn:hover {\n  background-color: var(--light-green);\n}\n\n/* Middle Section: Run by the Community */\n.runby-section {\n  background-color: var(--gradient-end);\n  padding: 30px 20px;\n  border-radius: 15px;\n  margin: 30px 0;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\n  text-align: center;\n}\n\n.runby-text {\n  color: var(--light-cream);\n  font-size: 1.8rem;\n  font-weight: bold;\n  margin-bottom: 15px;\n}\n\n.runby-description {\n  color: var(--light-cream);\n  font-size: 1rem;\n  line-height: 1.6;\n}\n\n/* Speakers Section */\n/* .speakers-section {\n    margin-top: 50px;\n}\n\n.speakers-title {\n    font-size: 2rem;\n    font-weight: 700;\n    color: var(--light-cream);\n    margin-bottom: 20px;\n}\n\n.speaker-card {\n    background-color: var(--dark-green);\n    border: 1px solid var(--light-green);\n    border-radius: 10px;\n    padding: 15px;\n    text-align: center;\n    color: var(--light-cream);\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\n}\n\n.speaker-card img {\n    border-radius: 50%;\n    margin-bottom: 15px;\n    max-width: 100px;\n    height: auto;\n} */\n\n/* Pricing Section */\n.pricing-section {\n  margin: 50px 0;\n  text-align: center;\n}\n\n.pricing-card {\n  background-color: var(--dark-green);\n  border: 2px solid var(--vibrant-green);\n  border-radius: 10px;\n  padding: 20px;\n  color: var(--light-cream);\n  margin: 10px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.3);\n}\n\n.pricing-card h3 {\n  color: var(--light-green);\n}\n\n.pricing-card:hover {\n  background-color: var(--light-green);\n  color: var(--dark-green);\n}\n\n/* Footer Section */\nfooter {\n  background-color: var(--dark-green);\n  color: var(--light-cream);\n  text-align: center;\n  padding: 30px 20px;\n  font-size: 0.9rem;\n  margin-top: 50px;\n}\n\n.footer-link {\n  color: black !important;\n  font-size: 1rem;\n  padding: 0.5rem 1rem !important;\n  text-decoratio